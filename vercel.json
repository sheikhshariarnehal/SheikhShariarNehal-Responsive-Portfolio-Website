{
  "version": 2,
  "functions": {
    "api/index.js": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api"
    },
    {
      "source": "/cms",
      "destination": "/api"
    },
    {
      "source": "/projects",
      "destination": "/api"
    },
    {
      "source": "/experience",
      "destination": "/api"
    },
    {
      "source": "/test-deployment.html",
      "destination": "/api"
    },
    {
      "source": "/",
      "destination": "/api"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
